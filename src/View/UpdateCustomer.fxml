<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="354.0" style="-fx-background-color: #2a2e37; -fx-border-color: #d9d9d6;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.UpdateCustomerController">
    <children>
        <TextField fx:id="custNameTxt" layoutX="126.0" layoutY="60.0" />
        <TextField fx:id="custPhoneTxt" layoutX="126.0" layoutY="105.0" />
        <TextField fx:id="custAddressTxt" layoutX="126.0" layoutY="151.0" />
        <TextField fx:id="custZipTxt" layoutX="126.0" layoutY="200.0" />
        <Label layoutX="54.0" layoutY="64.0" text="Name" textFill="WHITE">
         <font>
            <Font name="Sitka Small" size="12.0" />
         </font></Label>
        <Label layoutX="54.0" layoutY="109.0" text="Phone" textFill="WHITE">
         <font>
            <Font name="Sitka Small" size="12.0" />
         </font></Label>
        <Label layoutX="54.0" layoutY="155.0" text="Address" textFill="WHITE">
         <font>
            <Font name="Sitka Small" size="12.0" />
         </font></Label>
        <Label layoutX="54.0" layoutY="204.0" text="Zip Code" textFill="WHITE">
         <font>
            <Font name="Sitka Small" size="12.0" />
         </font></Label>
        <ComboBox fx:id="custCountryCombo" layoutX="125.0" layoutY="293.0" onAction="#divisionList" prefWidth="150.0" />
        <ComboBox fx:id="custDivCombo" layoutX="125.0" layoutY="339.0" prefWidth="150.0" />
        <Label layoutX="54.0" layoutY="297.0" text="Country" textFill="WHITE">
         <font>
            <Font name="Sitka Small" size="12.0" />
         </font></Label>
        <Label layoutX="54.0" layoutY="343.0" text="Division" textFill="WHITE">
         <font>
            <Font name="Sitka Small" size="12.0" />
         </font></Label>
        <Button fx:id="saveBtn" layoutX="82.0" layoutY="390.0" mnemonicParsing="false" onAction="#onSave" text="Save" />
        <Button fx:id="cancelBtn" layoutX="201.0" layoutY="390.0" mnemonicParsing="false" onAction="#onCancel" text="Cancel" />
        <Label layoutX="120.0" layoutY="14.0" text="Update Customer" textFill="WHITE">
            <font>
                <Font name="Sitka Small" size="18.0" />
            </font>
        </Label>
      <TextField fx:id="custIDTxt" disable="true" editable="false" layoutX="126.0" layoutY="247.0" />
      <Label layoutX="69.0" layoutY="251.0" text="ID" textFill="WHITE">
         <font>
            <Font name="Sitka Small" size="12.0" />
         </font></Label>
    </children>
</AnchorPane>
